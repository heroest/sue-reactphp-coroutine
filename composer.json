{
    "name": "sue/reactphp-coroutine",
    "description": "Coroutine Feature for ReactPHP's Eventloop",
    "keywords": ["event-loop", "asynchronous", "coroutine"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "heroest",
            "email": "kakashi@live.cn"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Sue\\Coroutine\\": "src/"
        },
        "files": [
            "src/function_include.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Sue\\Tests\\Coroutine\\": "tests/"
        },
        "files": [
            "src/function_include.php"
        ]
    },
    "require": {
        "php": "^7.1.3",
        "react/event-loop": "^1.1",
        "react/promise": "^2.8",
        "react/promise-timer": "^1.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0 || ^7.0"
    }
}
